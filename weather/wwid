import matplotlib.pyplot as plt

# Get the unique values for the bins (same for both columns)
unique_values = sorted(cohort_data['proposed_final_crr'].unique())

# Plotting
plt.figure(figsize=(12, 6))

# Plot histogram for 'proposed_final_crr' and 'CRR'
plt.hist(cohort_data['proposed_final_crr'], bins=len(unique_values), alpha=0.5, label='proposed_final_crr', color='blue')
plt.hist(cohort_data['CRR'], bins=len(unique_values), alpha=0.5, label='CRR', color='orange')

# Titles and labels
plt.title('Histogram of Frequencies of Proposed Final CRR and CRR')
plt.xlabel('Rating')
plt.ylabel('Frequency')

# Add legend
plt.legend()

# Show grid
plt.grid(True)

# Show the plot
plt.show()
